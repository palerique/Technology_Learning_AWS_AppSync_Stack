{
  "name": "technology_learning-aws_app_sync_stack",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "technology_learning-aws_app_sync_stack": "bin/technology_learning-aws_app_sync_stack.js"
  },
  "workspaces": [
    "lib/*",
    "lambda/*",
    "layer/*"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ..",
    "build": "yarn format:fix && yarn lint:fix && tsc --build --verbose",
    "watch": "tsc -w",
    "cleanup": "yarn wsrun rimraf dist/*",
    "format:test": "prettier --list-different '**/**/*.ts'",
    "format:fix": "prettier --write **/**/*.ts",
    "lint:test": "eslint --config .eslintrc.json --ext .ts,.tsx .",
    "lint:fix": "eslint --config .eslintrc.json --ext .ts,.tsx . --fix",
    "clean": "yarn cleanup && rm -rf node_modules",
    "test": "yarn wsrun jest --silent",
    "coverage": "yarn wsrun jest --coverage --silent"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.99.0",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.75",
    "@types/faker": "^5.5.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.2",
    "@types/qs": "^6.9.6",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "aws-cdk": "1.99.0",
    "babel-jest": "26.6.3",
    "babel-parser-minimal": "^1.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "jest-serial-runner": "^1.1.0",
    "npm-build-zip": "^1.0.3",
    "prettier": "^2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "wsrun": "^5.2.4"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "^1.99.0",
    "@aws-cdk/aws-dynamodb": "^1.99.0",
    "@aws-cdk/aws-iam": "^1.99.0",
    "@aws-cdk/aws-lambda": "^1.99.0",
    "@aws-cdk/aws-s3": "^1.99.0",
    "@aws-cdk/core": "1.99.0",
    "@aws-cdk/custom-resources": "^1.99.0",
    "aws-sdk": "^2.888.0",
    "faker": "^5.5.3",
    "randomquote-api": "^3.5.5",
    "source-map-support": "^0.5.16"
  }
}
