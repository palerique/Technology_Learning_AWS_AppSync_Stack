{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "lib": [
      "es2018"
    ],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "moduleResolution": "node",
    "composite": true,
    "incremental": true
  },
  "exclude": [
    "cdk.out"
  ],
//  "include": [
//    "lib/technology_learning-aws_app_sync_stack-stack.ts"
//  ],
//  "files": [],
  "references": [
    {
      "path": "./lambda/getComment/tsconfig.build.json"
    },
    {
      "path": "./lambda/deleteComment/tsconfig.build.json"
    },
    {
      "path": "./lambda/listComments/tsconfig.build.json"
    }
  ]
}
